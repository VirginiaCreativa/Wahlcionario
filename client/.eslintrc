{
  "extends": [
    "airbnb",
    "airbnb/hooks",
    "plugin:prettier/recommended",
    "eslint:recommended",
    "plugin:react/recommended",
    "prettier/react",
    "prettier/prettier",
    "prettier/standard",
    "plugin:compat/recommended",
    "plugin:cypress/recommended"
  ],
  "env": {
    "es6": true,
    "jest": true,
    "browser": true,
    "node": true,
    "jquery": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 8,
     "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "modules": true,
      "experimentalObjectRestSpread": true
    }
  },
  "plugins": ["react", "react-hooks", "jsx-a11y", "compat", "html", "jest", "prettier"],
  "rules": {
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [".js", ".jsx"]
      }
    ],
    "react-hooks/rules-of-hooks": "error",
    "react-hooks/exhaustive-deps": "warn",
    "no-console": "off",
    "react/prop-types": 0,
    "global-require": 0,
    "compat/compat": "off",
    "quotes": [
      2,
      "single",
      {
        "avoidEscape": true,
        "allowTemplateLiterals": true
      }
    ],
    "react/react-in-jsx-scope": 0,
    "react/jsx-props-no-spreading": 0,
    "import/no-unresolved": [
      2,
      { "caseSensitive": false }
    ],
    "camelcase": "off",
    "no-unused-vars": 0,
    "no-shadow": "off",
    "no-restricted-syntax": [
      2,
      "ForInStatement",
      "LabeledStatement",
      "WithStatement"
    ],
    "no-nested-ternary": 0,
    "no-underscore-dangle": 0,
    "jsx-a11y/mouse-events-have-key-events": 0,
    "no-return-assign": ["error", "except-parens"],
    "react/no-array-index-key": 0
  }
}